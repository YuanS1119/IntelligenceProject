.date{height: 40px;text-align: center;line-height: 40px;color: white;font-size: 16px;background: #07C160;}
.head{height: 25px;line-height: 25px;display: flex;background: #ddedff;padding: 0 25rpx;box-sizing: border-box;}
.head view{flex: 1;text-align: center;font-size: 13px;}
.box{width: 100%;padding: 0 25rpx;box-sizing: border-box;background: white;}
.box:after{clear: both;content: '';display: block;}
.date_item{width: 100rpx;float: left;text-align: center;height: 100rpx;line-height: 100rpx;position: relative;border-radius: 5px;}
.has_data{width: 6px;height: 6px;border-radius: 50%;background: #07C160;position: absolute;right: 10px;top: 10px;}
.checked{background: #07C160;color: white;border-radius: 50%;}
.date_item.today{font-size: 12px;}
.data_box{background: white;border-top: 1px solid #ddd;}
.data_title{height: 40px;line-height: 40px;padding-left: 20px;font-size: 16px;}

.lists{padding-left: 20px;}
.item{min-height: 25px;padding: 2px 0 2px 25px;line-height: 20px;position: relative;
    box-sizing: border-box;text-align: left;font-size: 14px;color: #333;}
.item image{display: block;position: absolute;left: 0;top: 50%;margin-top: -9px;width: 18px;height: 18px;}
.mt10{padding: 5px 40px 5px 0;position: relative;}
.mt10 .type{width: 32px;height: 32px;position: absolute;right: 5px;top: 50%;margin-top: -16px;}
.no_dk_data{padding: 30px 0;text-align: center;color: #666;}
.change_date{display: inline-block;width: 16px;height: 16px;position: relative;top: 3px;left: -2px;}

