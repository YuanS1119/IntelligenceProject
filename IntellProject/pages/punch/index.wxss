page{background: white;}
.head{height: 50px;padding-top: 20px;padding-right: 10px;}
.calendar{height: 26px;border-radius: 13px;border: 1px solid #ddd;padding: 0 10px 0 5px;
    float: right;line-height: 26px;font-size: 14px;color: #07C160;background: white;}
.calendar image{width: 26px;height: 26px;float: left;}

.address{margin-top: 50px;padding: 0 10%;text-align: center;}
.item{min-height: 40px;padding: 10px 0 10px 40px;line-height: 20px;position: relative;
    box-sizing: border-box;text-align: left;font-size: 16px;color: #333;margin-top: 10px;}
.item image{display: block;position: absolute;left: 0;top: 50%;margin-top: -13px;width: 26px;height: 26px;}
.time{font-size: 20px;font-family: '黑体';color: #666;}
.box{width: 80%;margin: 40px auto 0;background: white;padding: 30px 0;text-align: center;}
.dk{width: 80px;height: 80px;border-radius: 50px;text-align: center;display: inline-block;padding: 5px 0;
    background: white;border: 6px solid #d0d7dd;line-height: 20px;font-size: 14px;margin: 0 30px;color: #666}
.dk image{width: 30px;height: 30px;display: block;margin: 0 auto 5px;}

.last .title{height: 30px;line-height: 30px;font-size: 16px;color: #666;margin-bottom: 10px;}
.item.item1{margin-top: 0;min-height: 30px;padding: 5px 0 5px 40px}
.item.item1 image{margin-top: -10px;width: 20px;height: 20px;}
.item.item1 .time{font-size: 14px;}