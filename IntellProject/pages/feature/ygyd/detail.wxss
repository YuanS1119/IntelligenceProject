.title{box-sizing: border-box;padding: 15px 10px;text-align: center;font-size: 17px;
    line-height: 25px;background: white;border-bottom: 1px solid #ddd;}
.error{height: 60px;line-height: 60px;text-align: center;}
.box{margin-bottom: 10px;}


.image_detail{position: fixed;width: 100%;height: 0;left: 0;top: 100%;z-index: 11;
    background: rgba(33,33,33,0.8);
    align-items: center;/** 垂直居中*/
    display: flex;
    justify-content: center;}
.image_detail_box{
    align-items: center;/** 垂直居中*/
    display: flex;
    justify-content: center;width: 100%;min-height: 100%;
}
.image_detail_info{max-width: 100%;display: block;width: 100%;height: auto;}

.image{padding: 20rpx 30rpx;box-sizing: border-box;}
.image image{display: block;max-height: 200rpx;margin: 0 auto;width: auto}